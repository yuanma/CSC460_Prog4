<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
     PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>UACATS.servlets.LoginServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>AddArtifactServlet</servlet-name>
		<servlet-class>UACATS.servlets.AddArtifactServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>FindArtifactServlet</servlet-name>
		<servlet-class>UACATS.servlets.FindArtifactServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AddArtifactServlet</servlet-name>
		<url-pattern>/AddArtifactServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FindArtifactServlet</servlet-name>
		<url-pattern>/FindArtifactServlet</url-pattern>
	</servlet-mapping>
	
</web-app>
